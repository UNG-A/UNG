#include<stdio.h> 
#include<stdlib.h> 



		







int main() {

	int prime[1024];
	int index = 0;
	prime[index++] = 2;
	prime[index++] = 3;

	 // prime number .
	for (int n = 5; n < 1000; n += 2)
	{
		for (int j = 0 ; prime[index] * prime[index] < n ; j++) {

			if (n % prime[j] == 0)
				break;

		}
		prime[index++] = n;

	}

	int i = 0;
	while (i < index) {

		printf("%4d", prime[i]);
		i++;
	}

}
